<h3>Upload a photo</h3>
<%= form_for @photo do |f| %>
<%= render 'shared/error_messages', object: f.object %>
<%= f.file_field :image %>
<%= f.hidden_field :lon, value: @longitude %>
<%= f.hidden_field :lat, value: @latitude %>
<%= f.submit "Upload Photo" %>
<% end %>

<div id="Latitude"><%= @latitude %></div>
<div id="longitude"><%= @longitude %></div>